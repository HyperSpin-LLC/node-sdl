{
  "version": "0.0.16",
  "name": "@kmamal/sdl",
  "description": "SDL2 bindings for Node.js",
  "keywords": [
    "sdl",
    "sdl2",
    "gui",
    "windows",
    "screen",
    "video",
    "audio",
    "sound",
    "input",
    "mouse",
    "keyboard",
    "clipboard",
    "game"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/kmamal/node-sdl.git"
  },
  "main": "src/index.js",
  "gypfile": true,
  "scripts": {
    "test": "tests/run-all.sh",
    "rebuild": "node-gyp rebuild --verbose",
    "prebuild": "prebuild --runtime napi --strip --verbose",
    "install": "prebuild-install --runtime napi || node-gyp rebuild --verbose"
  },
  "binary": {
    "napi_versions": [
      3
    ]
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "prebuild-install": "^6.1.2"
  },
  "devDependencies": {
    "prebuild": "^10.0.1"
  }
}