{
  "version": "0.4.5",
  "name": "@kmamal/sdl",
  "description": "SDL2 bindings for Node.js",
  "keywords": [ "sdl", "sdl2", "gui", "window", "screen", "video", "audio", "sound", "input", "mouse", "keyboard", "clipboard", "game" ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/kmamal/node-sdl.git"
  },
  "main": "src/javascript/index.js",
  "types": "src/types/index.d.ts",
  "gypfile": true,
  "scripts": {
    "test": "cd tests; node run-all.js",
    "build-lib": "npx node-gyp rebuild --verbose",
    "build-sdl": "cd scripts; node build-sdl.js",
    "publish": "cd scripts; node upload-bins.js",
    "install": "cd scripts; node fetch-bins.js || (node fetch-deps.js && npm run build-lib)"
  },
  "dependencies": {
    "bindings": "^1.5.0"
  },
  "devDependencies": {
    "node-gyp": "^9.3.0"
  }
}
