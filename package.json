{
  "version": "0.5.0",
  "name": "@kmamal/sdl",
  "description": "SDL2 bindings for Node.js",
  "keywords": [
    "sdl",
    "sdl2",
    "gui",
    "window",
    "screen",
    "video",
    "audio",
    "sound",
    "input",
    "mouse",
    "keyboard",
    "clipboard",
    "game"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/kmamal/node-sdl.git"
  },
  "main": "src/javascript/index.js",
  "types": "src/types/index.d.ts",
  "gypfile": true,
  "scripts": {
    "test": "cd scripts && npx -y zx test.mjs",
    "install": "cd scripts && npx -y zx install.mjs",
    "download-release": "cd scripts && npx -y zx download-release.mjs",
    "build": "cd scripts && npx -y zx build.mjs",
    "upload-release": "cd scripts && npx -y zx upload-release.mjs",
    "release": "cd scripts && npx -y zx release.mjs",
    "clean": "cd scripts && npx -y zx clean.mjs",
    "install-deps-mac": "./scripts/install-deps-mac.sh"
  },
  "dependencies": {
    "@kmamal/build-sdl": "~2.24.1-3"
  },
  "devDependencies": {
    "node-gyp": "^9.3.0"
  }
}
