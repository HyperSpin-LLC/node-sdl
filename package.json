{
  "version": "0.0.10",
  "name": "@kmamal/sdl",
  "description": "SDL2 bindings for Node.js",
  "keywords": [
    "sdl",
    "sdl2",
    "gui",
    "video",
    "audio",
    "input"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kmamal/node-sdl.git"
  },
  "main": "src/index.js",
  "scripts": {
    "rebuild": "node-gyp rebuild --verbose",
    "prebuild": "prebuild --all --strip -r napi -t 3",
    "install": "prebuild-install || node-gyp rebuild"
  },
  "gypfile": true,
  "binary": {
    "napi_versions": [
      3
    ]
  },
  "license": "ISC",
  "dependencies": {
    "bindings": "*",
    "prebuild": "^10.0.1"
  }
}